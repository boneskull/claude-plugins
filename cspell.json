{
  "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/packages/cspell-types/cspell.schema.json",
  "allowCompoundWords": true,
  "caseSensitive": false,
  "dictionaries": [
    "companies",
    "css",
    "en_US",
    "filetypes",
    "fonts",
    "html",
    "misc",
    "node",
    "npm",
    "softwareTerms",
    "typescript"
  ],
  "ignorePaths": [
    ".coverage",
    ".tmp",
    ".vscode",
    "*.snapshot",
    "*.svg",
    "CHANGELOG.md",
    "coverage",
    "dist",
    "docs",
    "node_modules",
    ".worktrees",
    ".git",
    ".husky"
  ],
  "ignoreRandomStrings": true,
  "ignoreWords": [
    "boneskull",
    "wagoid",
    "zshy",
    "toshimaru",
    "pids",
    "jscoverage",
    "wscript",
    "bahmutov",
    "huskyrc",
    "commitlintrc",
    "clune",
    "obra",
    "myorg",
    "sindresorhus"
  ],
  "words": [
    "viewports",
    "shankle",
    "picanha",
    "bupkis",
    "andouille",
    "signup",
    "treeify",
    "Luhn",
    "unpatch",
    "journalctl",
    "stringifying"
  ]
}
